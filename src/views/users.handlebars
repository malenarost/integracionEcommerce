<main>
  <div class="container-fluid">
    <div class="row">
      <div class="col g-0">
        <div class="bg-image" style="filter: blur(5px);"></div>

        <div class="contenedorTituloPrincipal">
          <a href="/"><img src="/images/logo.png" class="logo" alt="" /></a>
          <h2 class="tituloPrincipal">burguers a la parrilla</h2>
          <table
            class="table table-dark table-striped-columns mt-3 w-50 text-center"
          >
            <thead>
              <tr class="table-secondary">
                <th scope="col">ID</th>
                <th scope="col">EMAIL</th>
                <th scope="col">USERNAME</th>
                <th scope="col">PASSWORD</th>
                <th scope="col">ROL</th>
              </tr>
            </thead>
            {{#each dataParse}}
              <tbody>
                <tr>
                  <td>{{this.id}}</td>
                  <td>{{this.email}}</td>
                  <td>{{this.username}}</td>
                  <td>{{this.password}}</td>
                  <td>{{this.rol}}</td>
                </tr>
              </tbody>
            {{/each}}
          </table>
        </div>
      </div>
    </div>
  </div>
</main>